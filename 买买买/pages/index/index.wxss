/**index.wxss**/
.index-swiper > swiper {
  width: 750rpx;
  height: 340rpx;
}
.index-swiper > swiper image {
  width: 100%;
}

.index-cate {
  display: flex;
  padding: 20rpx;
}
.index-cate navigator {
  flex: 1;
}
.index-cate image {
  width: 100%;
}

/* .index-floor {} */
.floor-group {
  margin-top: 20rpx;
}
.floor-title image {
  width: 100%;
}

.floor-list {
  display: grid;
  grid-template-columns: 1fr 1fr 1fr;
  grid-template-rows: 200rpx 200rpx;
  place-items: stretch / stretch;
  grid-gap: 10rpx;
}
.floor-list > navigator {
  width: 100%;
  height: 100%;
}
.floor-list > navigator:first-child {
  grid-row: 1 / span 2;
}
.floor-list image {
  width: 100%;
  height: 100%;
}
